<?xml version="1.0"?>
<launch>
  
  <arg name="model" default="finder"/>
  <arg name="x_pos" default="2.5030"/>
  <arg name="y_pos" default="8.5555"/>
  <arg name="z_pos" default="0.0"/>
  <arg name="yaw_value"   default="-1.570403"/>


  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find finder_gazebo)/worlds/apartment_fast_base.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>  


  <param name="robot_description" command="$(find xacro)/xacro.py $(find finder_description)/robot/finder_base_8w.urdf.xacro" />

  <!-- Launch control launch file -->
  <include file="$(find finder_gazebo)/launch/finder_base_control.launch"/>

  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-urdf -model $(arg model) -x $(arg x_pos) -y $(arg y_pos) -z $(arg z_pos) -Y $(arg yaw_value) -param robot_description" />


  
</launch>
